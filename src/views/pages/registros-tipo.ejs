    </div>

    <%- include('../components/registros', { table: body.table }); %>

    <% if (tipo != "Adm") { %>
            
        <div class="center">
            <%- include('../components/box-delete', { label: "Confirme a exclusÃ£o da classe:" , button: "Deletar" ,
                route: `/registro/deletar-tipo/${tipo}`, redirect: "/registros" }) %>
        </div>
    
    <% } %>

<% } else { %>

    <script>
        window.location.replace("/");
